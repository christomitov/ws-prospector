# Optional: force a specific signing identity (otherwise auto-detected)
# APPLE_SIGN_IDENTITY="Developer ID Application: YOUR NAME (TEAMID)"

# Optional: custom notary profile name (default: ws-notary)
APPLE_NOTARY_PROFILE=ws-notary

# Required for automatic notarization profile creation on first run
APPLE_ID=your-apple-id@example.com
APPLE_TEAM_ID=2H56V7T355
APPLE_APP_PASSWORD=xxxx-xxxx-xxxx-xxxx

# Minimum supported macOS version for launcher/plist metadata
WSP_MACOS_DEPLOYMENT_TARGET=11.0

# Sparkle auto-update (required for in-app updates)
WSP_SPARKLE_VERSION=2.9.0
WSP_SPARKLE_FEED_URL=https://christomitov.github.io/ws-prospector/appcast.xml
# Generate with Sparkle's `bin/generate_keys` and keep private key out of git.
WSP_SPARKLE_PUBLIC_KEY=
